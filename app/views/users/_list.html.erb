<div class='habit_list user_habit_list'>
  <div class='habit_list_top'>
    <div class='habit_list_left_btn'>
      <div class='habit_title'>
      <%= list.title %>
      </div>
    </div>
    <div class='habit_list_right_btn'>
      <%= link_to edit_habit_path(list.id),  class: "link" do %>
      <div class='habit_list_edit_delete'>
        <div class='habit_edit_delete_btn'>
        編集
        </div>
      </div>
      <% end %>
      <%= link_to habit_path(list.id), method: :delete,  class: "link" do %>
      <div class='habit_list_edit_delete'>
        <div class='habit_edit_delete_btn'>
        削除
        </div>
      </div>
      <% end %>
    </div>
  </div>
  <div class='habit_list_bottom'>
  <ul>
  <li class='habit_item'><%= list.item1 %></li>
  <li class='habit_item'><%= list.item2 %></li>
  <li class='habit_item'><%= list.item3 %></li>
  <li class='habit_item'><%= list.item4 %></li>
  <li class='habit_item'><%= list.item5 %></li>
  </ul>
  </div>
</div>
